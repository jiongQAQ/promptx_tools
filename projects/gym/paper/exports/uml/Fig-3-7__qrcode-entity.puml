@startuml
!define RECTANGLE class
!theme plain

entity QRCode {
  * id : Long <<PK, AUTO_INCREMENT>>
  --
  * qr_id : String <<UNIQUE, NOT NULL>>
  * venue_id : Long <<FK>>
  * qr_content : String <<NOT NULL>>
  qr_code_data : String <<BASE64>>
  valid_hours : Integer
  size : Integer
  format : String
  background_color : String
  foreground_color : String
  with_logo : Integer <<0-无LOGO,1-有LOGO>>
  valid_start_time : LocalDateTime
  valid_end_time : LocalDateTime
  usage_count : Integer <<DEFAULT 0>>
  status : Integer <<1-有效,2-已失效,0-已撤销>>
  created_by : Long <<FK>>
  create_time : LocalDateTime <<AUTO_FILL>>
  expire_time : LocalDateTime
  is_active : Integer <<1-激活,0-未激活>>
  is_delete : Integer <<LOGICAL_DELETE>>
}

note right of QRCode : 二维码实体类\n管理场地和预约二维码\n支持动态生成和有效期控制

@enduml