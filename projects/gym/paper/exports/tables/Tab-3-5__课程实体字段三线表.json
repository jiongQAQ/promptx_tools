[
  {
    "字段名": "id",
    "类型": "BIGINT",
    "约束": "主键、自增、非空",
    "说明": "课程唯一标识符"
  },
  {
    "字段名": "name",
    "类型": "VARCHAR(100)",
    "约束": "非空",
    "说明": "课程名称（如瑜伽、动感单车等）"
  },
  {
    "字段名": "description",
    "类型": "TEXT",
    "约束": "",
    "说明": "课程详细描述和内容介绍"
  },
  {
    "字段名": "instructor_id",
    "类型": "BIGINT",
    "约束": "外键、非空",
    "说明": "授课教练ID，关联用户表"
  },
  {
    "字段名": "venue_id",
    "类型": "BIGINT",
    "约束": "外键、非空",
    "说明": "上课场地ID，关联场地表"
  },
  {
    "字段名": "start_time",
    "类型": "DATETIME",
    "约束": "非空",
    "说明": "课程开始时间"
  },
  {
    "字段名": "end_time",
    "类型": "DATETIME",
    "约束": "非空",
    "说明": "课程结束时间"
  },
  {
    "字段名": "max_participants",
    "类型": "INT",
    "约束": "非空、默认值20",
    "说明": "课程最大参与人数"
  },
  {
    "字段名": "current_participants",
    "类型": "INT",
    "约束": "非空、默认值0",
    "说明": "当前已报名人数"
  },
  {
    "字段名": "fee",
    "类型": "DECIMAL(10,2)",
    "约束": "非空、默认值0.00",
    "说明": "课程费用"
  },
  {
    "字段名": "status",
    "类型": "VARCHAR(20)",
    "约束": "默认值SCHEDULED",
    "说明": "课程状态：SCHEDULED-已安排 ONGOING-进行中 COMPLETED-已结束 CANCELLED-已取消"
  },
  {
    "字段名": "level",
    "类型": "VARCHAR(20)",
    "约束": "默认值BEGINNER",
    "说明": "课程难度：BEGINNER-初级 INTERMEDIATE-中级 ADVANCED-高级"
  },
  {
    "字段名": "category",
    "类型": "VARCHAR(50)",
    "约束": "",
    "说明": "课程类别（如有氧运动、力量训练、瑜伽等）"
  },
  {
    "字段名": "create_time",
    "类型": "DATETIME",
    "约束": "默认当前时间",
    "说明": "课程创建时间"
  },
  {
    "字段名": "update_time",
    "类型": "DATETIME",
    "约束": "自动更新",
    "说明": "最后更新时间"
  },
  {
    "字段名": "is_delete",
    "类型": "TINYINT",
    "约束": "默认值0",
    "说明": "是否删除：0-未删除 1-已删除"
  }
]