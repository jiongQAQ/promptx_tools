[
  {
    "字段名": "id",
    "类型": "BIGINT",
    "约束": "主键、自增、非空",
    "说明": "场地唯一标识符"
  },
  {
    "字段名": "name",
    "类型": "VARCHAR(100)",
    "约束": "非空",
    "说明": "场地名称（如篮球场1号、瑜伽室A等）"
  },
  {
    "字段名": "description",
    "类型": "TEXT",
    "约束": "",
    "说明": "场地详细描述和特色介绍"
  },
  {
    "字段名": "location",
    "类型": "VARCHAR(255)",
    "约束": "",
    "说明": "场地具体位置（楼层、区域等）"
  },
  {
    "字段名": "equipment",
    "类型": "TEXT",
    "约束": "",
    "说明": "场地配置的设备清单"
  },
  {
    "字段名": "capacity",
    "类型": "INT",
    "约束": "非空、默认值0",
    "说明": "场地最大容纳人数"
  },
  {
    "字段名": "hourly_rate",
    "类型": "DECIMAL(10,2)",
    "约束": "非空、默认值0.00",
    "说明": "场地小时收费标准"
  },
  {
    "字段名": "status",
    "类型": "VARCHAR(20)",
    "约束": "默认值AVAILABLE",
    "说明": "场地状态：AVAILABLE-可用 MAINTENANCE-维护中 DISABLED-停用"
  },
  {
    "字段名": "create_time",
    "类型": "DATETIME",
    "约束": "默认当前时间",
    "说明": "场地创建时间"
  },
  {
    "字段名": "update_time",
    "类型": "DATETIME",
    "约束": "自动更新",
    "说明": "最后更新时间"
  },
  {
    "字段名": "is_delete",
    "类型": "TINYINT",
    "约束": "默认值0",
    "说明": "是否删除：0-未删除 1-已删除"
  }
]